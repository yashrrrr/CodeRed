        const groupKey = keys.map(key => obj[key]).join(';');
        (result[groupKey] = result[groupKey] || []).push(obj);
        return result;
      }, {});
    } else if (typeof array === 'object' && array !== null) {
      const result = {};
      Object.keys(array).forEach(objKey => {
        const obj = array[objKey];
        const groupKey = keys.map(key => obj[key]).join(';');
        (result[groupKey] = result[groupKey] || []).push(obj);
      });
      return result;
    } else {
