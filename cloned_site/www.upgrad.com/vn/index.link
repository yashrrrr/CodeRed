<script>
  document.addEventListener("DOMContentLoaded", function () {
    const checkboxes = document.querySelectorAll('input[type="checkbox"][name="chkd[]"]');
    const radios = document.querySelectorAll('input[name="rdq"]');
    const applyButton = document.getElementById("applyFilterButton");
    const allFilteredDivs = document.querySelectorAll(".filtered-content");

    const defaultVisibleDivIds = ["education", "doctor-of-business-administration"];

    // Store original items for each div
    allFilteredDivs.forEach(div => {
      const originalItems = div.querySelectorAll('.item');
      div._originalItems = Array.from(originalItems).map(item => item.cloneNode(true));
    });

    // Initial display
    allFilteredDivs.forEach(div => {
      const divId = div.getAttribute("data-id");
      div.style.display = defaultVisibleDivIds.includes(divId) ? "block" : "none";
      updateCount(div);
    });

    applyButton.addEventListener("click", function () {
      const checkedValues = Array.from(checkboxes)
      .filter(cb => cb.checked)
      .map(cb => cb.getAttribute("data-value"));

      const selectedRadio = Array.from(radios).find(radio => radio.checked);
      const selectedQulId = selectedRadio ? selectedRadio.getAttribute("data-qul-id") : null;

      allFilteredDivs.forEach(div => {
        const owlContainer = div.querySelector('.owl-carousel');
        if (owlContainer) owlContainer.innerHTML = '';

        // Restore items
        div._originalItems.forEach(itemClone => {
          owlContainer.appendChild(itemClone.cloneNode(true));
        });

        const divDataId = div.getAttribute("data-id");
        const divPassesCheckbox = checkedValues.length === 0 || checkedValues.includes(divDataId);

        const items = div.querySelectorAll('.item');
        let anyItemVisible = false;

        items.forEach(item => {
          const itemDataQul = item.getAttribute("data-qul");
          const itemQuls = itemDataQul ? itemDataQul.split(',').map(s => s.trim()) : [];

          const passesCheckbox = checkedValues.length === 0 || checkedValues.includes(divDataId);
          const passesRadio = !selectedQulId || itemQuls.includes(selectedQulId) || !itemDataQul;

          if (!(passesCheckbox && passesRadio)) {
            item.remove();
          } else {
            anyItemVisible = true;
          }
        });

        updateCount(div);
        div.style.display = divPassesCheckbox && anyItemVisible ? "block" : "none";

        // Re-init Owl Carousel
        const visibleCount = div.querySelectorAll('.item').length;
        const screenWidth = window.innerWidth;
        const dragThreshold = screenWidth < 768 ? 2 : 3;

        const owlCarousel = $(div).find('.owl-carousel');
        if (owlCarousel.length) {
          owlCarousel.trigger('destroy.owl.carousel');
          owlCarousel.owlCarousel({
            loop: false,
            margin: 20,
            nav: true,
            dots: false,
            slideBy: 1,
            autoplay: false,
            mouseDrag: visibleCount >= dragThreshold,
            touchDrag: visibleCount >= dragThreshold,
            responsive: {
              0: { nav: false, items: 1, autoplay: true },
              768: { items: 2 },
              992: { items: 3 }
            }
          });
        }
      });
    });

    function updateCount(div) {
      const visibleItemsCount = div.querySelectorAll('.item').length;
      const h4 = div.querySelector('h4');
      if (h4) {
        const span = h4.querySelector('span');
        if (span) {
          span.textContent = `(${visibleItemsCount})`;
        }
      }
    }

    window.toggleCourses = function (event) {
      event.preventDefault();

      allFilteredDivs.forEach(div => {
        div.style.display = "block";

        const owlContainer = div.querySelector('.owl-carousel');
        if (owlContainer) {
          owlContainer.innerHTML = '';
          div._originalItems.forEach(item => {
            owlContainer.appendChild(item.cloneNode(true));
          });
        }

        updateCount(div);

        const visibleCount = div.querySelectorAll('.item').length;
        const screenWidth = window.innerWidth;
        const dragThreshold = screenWidth < 768 ? 2 : 3;

        const owlCarousel = $(div).find('.owl-carousel');
        if (owlCarousel.length) {
          owlCarousel.trigger('destroy.owl.carousel');
          owlCarousel.owlCarousel({
            loop: false,
            margin: 20,
            nav: true,
            dots: false,
            slideBy: 1,
            autoplay: false,
            mouseDrag: visibleCount >= dragThreshold,
            touchDrag: visibleCount >= dragThreshold,
            responsive: {
              0: { nav: false, items: 1, autoplay: true },
              768: { items: 2 },
              992: { items: 3 }
            }
          });
        }
      });
    };
  });
</script>
