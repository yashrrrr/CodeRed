      function parseStringToJson2(input) {
        // Step 1: Convert = to : for key-value pairs
        let fixed = input.replace(/([a-zA-Z0-9_]+)\s*=/g, '"$1":');

        // Step 2: Ensure all keys are quoted
        fixed = fixed.replace(/([{,\[]\s*)([a-zA-Z0-9_]+)\s*:/g, '$1"$2":');
        fixed = fixed.replace(/^(\s*)([a-zA-Z0-9_]+)\s*:/, '$1"$2":');

        // Step 3: Fix values - handle true, false, null, numbers, and strings
        fixed = fixed.replace(/:\s*([^"{\[\],\s][^,\]}]*?)(?=\s*[,}\]])/g, (match, p1) => {
          const val = p1.trim();
          if (val === 'true') return ': true';
          if (val === 'false') return ': false';
          if (val === 'null') return ': null';
          if (/^-?\d+(\.\d+)?([eE][+-]?\d+)?$/.test(val)) return `: ${val}`;
          return `: "${val.replace(/"/g, '\\"')}"`;
        });

        // Step 4: Handle empty values
        fixed = fixed.replace(/:\s*(?=[,\]}])/g, ': null');

        // Step 5: Fix arrays with mixed content
        fixed = fixed.replace(/\[([^\[\]]*?)\]/g, (match, p1) => {
          if (p1.trim() === '') return '[]';
          const items = p1.split(',').map(item => {
            const val = item.trim();
            if (val === '') return 'null';
            if (/^".*"$/.test(val)) return val;
            if (/^(true|false|null|-?\d+(\.\d+)?)$/.test(val)) return val;
            return `"${val.replace(/"/g, '\\"')}"`;
          });
          return `[${items.join(',')}]`;
        });

        // Step 6: Add missing commas between properties
        fixed = fixed.replace(/"\s*"\s*:/g, '", "":');
        fixed = fixed.replace(/([}\]"])\s*(["{[]|"[\w_]+":)/g, '$1, $2');

        // Step 7: Handle specific problematic cases in your data
        fixed = fixed.replace(/"AMBA, EFMD Accredited"/g, '"AMBA, EFMD Accredited"');
        fixed = fixed.replace(/"([^"]*)\s*"\s*:/g, '"$1":'); // Fix keys with trailing spaces

        // Debugging: Output the area around the error position
        const errorPos = 13382;
        console.log('Context around error position:',
                    fixed.substring(errorPos - 50, errorPos + 50));

        try {
          return JSON.parse(fixed);
        } catch (err) {
          console.error('❌ JSON parse failed:', err.message);
          console.error('Error context:', fixed.substring(err.position - 50, err.position + 50));
          return null;
        }
      }

      function fixPseudoJson(input) {
        let fixed = input;

        // 1. Remove newlines
        fixed = fixed.replace(/[\r\n]+/g, ' ');

        // 2. Quote keys (this works safely)
        fixed = fixed.replace(/([{,]\s*)([a-zA-Z0-9_]+)\s*=/g, '$1"$2":');
        fixed = fixed.replace(/^([a-zA-Z0-9_]+)\s*=/, '"$1":');

        // 3. Fix arrays: ensure all array items are quoted
        fixed = fixed.replace(/\[([^\[\]]*?)\]/g, (match, p1) => {
          const items = p1.split(',').map(item => {
            const val = item.trim();
            if (!val) return 'null';
            if (/^".*"$/.test(val) || /^(true|false|null|-?\d+(\.\d+)?)$/.test(val)) return val;
            return `"${val.replace(/"/g, '\\"')}"`;
          });
          return `[${items.join(',')}]`;
        });

        // 4. Fix unquoted single string values (the key bug):
        fixed = fixed.replace(/:\s*([^"{\[\],\s][^,\]}]*)/g, (match, p1) => {
          const val = p1.trim();
          if (/^(true|false|null|-?\d+(\.\d+)?)$/.test(val)) return `: ${val}`;
          return `: "${val.replace(/"/g, '\\"')}"`;
        });

        // 5. Add missing commas
        fixed = fixed.replace(/}\s*{/g, '}, {');
        fixed = fixed.replace(/}\s*\]/g, '} ]');
        fixed = fixed.replace(/]\s*{/g, '], {');

        // 6. Fix missing values
        fixed = fixed.replace(/:\s*(?=[,\]}])/g, ': null');

        // 7. Clean up commas
        fixed = fixed.replace(/,,+/g, ',');

        // 8. Ensure braces
        fixed = fixed.trim();
        if (!fixed.startsWith('{')) fixed = `{${fixed}}`;

        try {
          return JSON.parse(fixed);
        } catch (error) {
          console.error('❌ Parse failed:', error.message);
          const position = error.position || 0;
          console.error(fixed.substring(Math.max(position - 100, 0), position + 100));
          return null;
        }
      }





      function combineArrays(arr1, arr2) {
        const combined = [];

        // Create a map to keep track of objects by their 'id'
        const map = new Map();

        // Add objects from the first array to the map
        arr1.forEach(item => {
          map.set(item.id, { ...item });
        });

        // Add objects from the second array, merging if 'id' exists
        arr2.forEach(item => {
          if (map.has(item.id)) {
            map.set(item.id, { ...map.get(item.id), ...item });
          } else {
            map.set(item.id, { ...item });
          }
        });

        // Convert the map back to an array
        map.forEach(value => {
          combined.push(value);
        });

        return combined;
      }


