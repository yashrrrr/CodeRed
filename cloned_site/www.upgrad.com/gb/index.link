<script>
document.addEventListener("DOMContentLoaded", function () {
    setTimeout(() => {
        const counters = document.querySelectorAll('.counter');
        const speed = 20;

        const isInViewport = (el) => {
            const rect = el.getBoundingClientRect();
            return (
                rect.top >= 0 &&
                rect.bottom <= (window.innerHeight || document.documentElement.clientHeight)
            );
        };

        let started = false;

        function runCounters() {
            if (started) return;

            counters.forEach(counter => {
                let rawValue = counter.getAttribute('data-count') || '0';
                rawValue = rawValue.replace(/,/g, '').trim();
                const target = parseFloat(rawValue);
                if (isNaN(target)) {
                    console.warn('Invalid number for counter:', rawValue);
                    counter.innerText = '0';
                    return;
                }

                const updateCount = () => {
                    const current = parseFloat(counter.innerText.replace(/,/g, '')) || 0;
                    const increment = Math.max(target / speed, 1);

                    if (current < target) {
                        const next = Math.min(current + increment, target);
                        counter.innerText = Math.floor(next).toLocaleString();
                        setTimeout(updateCount, 10);
                    } else {
                        counter.innerText = Math.floor(target).toLocaleString();
                    }
                };

                updateCount();
            });

            started = true;
        }

        function checkScroll() {
            const section = document.getElementById('unique-value-proposition');
            if (section && isInViewport(section)) {
                runCounters();
                window.removeEventListener('scroll', checkScroll);
            }
        }

        window.addEventListener('scroll', checkScroll);
        checkScroll();
    }, 5000); // 1000 ms = 1 seconds
});
</script>



