        var dropdownBtn = document.getElementById("dropdown-btn2");
        var radioInputs = document.querySelectorAll('input[name="rd1"]');
        
        radioInputs.forEach(function(input) {
            input.addEventListener('change', function() {
                if (this.checked) {
                    dropdownBtn.innerHTML = this.nextElementSibling.innerHTML;
                } else {
                    dropdownBtn.innerHTML = "Select your highest qualification"; // Clear the dropdown if no radio button is checked
                }
            });
            
            // Add click event listener to toggle radio button state
            input.addEventListener('click', function() {
                if (this.checked) {
                    if (this.getAttribute('data-checked') === 'true') {
                        this.checked = false; // Uncheck if already checked
                        dropdownBtn.innerHTML = "Select your highest qualification"; // Clear the dropdown
                        this.setAttribute('data-checked', 'false');
                    } else {
                        this.setAttribute('data-checked', 'true');
                    }
                }
            });
        });
    });
  
 
 function updateDropdownText() {
    // Only select checkboxes inside the dropdown
    const checkboxes = document.querySelectorAll('.select-box1 .dropdown-menu input[type="checkbox"]:checked');
    
    const selectedValues = new Set();
    const maxLength = 500;

    checkboxes.forEach(function(checkbox) {
        let label = checkbox.nextElementSibling.textContent.trim().replace(/\s+/g, ' ');
        label = label.replace(/\(.+?\)/g, '');
        selectedValues.add(label.trim());
    });

    let displayValues = Array.from(selectedValues).slice(0, 2).join(', ');
    let additionalCount = selectedValues.size - 2;
