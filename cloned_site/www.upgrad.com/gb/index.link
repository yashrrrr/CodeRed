         const element = document.querySelector(selector);
         if (element) {
           return resolve(element);
         }
         const observer = new MutationObserver((mutations) => {
           const element = document.querySelector(selector);
           if (element) {
             resolve(element);
             observer.disconnect();
           }
         });
         observer.observe(document.documentElement, {
           childList: true,
           subtree: true,
         });
       });
