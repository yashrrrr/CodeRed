<script>
  document.addEventListener("DOMContentLoaded", function() {
    // Get all elements with the class "updateable"
    var updateableElements = document.querySelectorAll(".updateable");

    // Function to update the data-val attribute based on the count
    function updateDataVal(element) {
      var count = parseInt(element.textContent.match(/\((\d+)\)/)[1]);
      element.dataset.val = count;
    }

    // Initial update for each element
    updateableElements.forEach(function(element) {
      updateDataVal(element);
    });

    // Create a MutationObserver to watch for changes in the DOM
    var observer = new MutationObserver(function(mutationsList) {
      mutationsList.forEach(function(mutation) {
        // Check if the content of the observed element has changed
        if (mutation.type === 'childList' && mutation.target.classList.contains('updateable')) {
          // Update the data-val attribute
          updateDataVal(mutation.target);
        }
      });
    });

    // Configure the observer to watch for changes to the child nodes
    var config = { childList: true, subtree: true };
    // Start observing changes on each updateable element
    updateableElements.forEach(function(element) {
      observer.observe(element, config);
    });
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    // Add event listeners to checkboxes
    var checkboxes = document.querySelectorAll('input[type="checkbox"]');
    checkboxes.forEach(function (checkbox) {
      checkbox.addEventListener('change', updateCount);
    });

    // Add event listeners to radio buttons
    var radioButtons = document.querySelectorAll('input[type="radio"]');
    radioButtons.forEach(function (radioButton) {
      radioButton.addEventListener('change', updateCount);
      radioButton.addEventListener('click', updateCount);
    });

    // Add event listener to the "Clear All" button
    var clearAllButton = document.getElementById('clearAllButton');
    clearAllButton.addEventListener('click', clearAll);

    // Function to update the count in the span for the specific tab
    function updateCount() {
      // console.log('Updating count...');

      var countSpans = document.querySelectorAll('.count-badge');
      countSpans.forEach(function (countSpan) {
        var tabId = countSpan.getAttribute('data-tab');
        //console.log('Tab ID:', tabId);

        // Select checkboxes and radios within the current tab
        var tabCheckboxes = document.querySelectorAll('#v-pills-' + tabId + ' input[type="checkbox"]:checked');
        var tabRadios = document.querySelectorAll('#v-pills-' + tabId + ' input[type="radio"]:checked');
        // console.log('Tab Checkboxes:', tabCheckboxes.length);
        // console.log('Tab Radios:', tabRadios.length);

        var totalCount = tabCheckboxes.length + tabRadios.length;
        //console.log('Total Count:', totalCount);

        countSpan.textContent = '(' + totalCount + ')';
      });
    }

    // Function to clear all checkboxes and radios and reset the count
    function clearAll() {
      var checkboxes = document.querySelectorAll('input[type="checkbox"]');
      checkboxes.forEach(function (checkbox) {
        checkbox.checked = false;
      });

      var radios = document.querySelectorAll('input[type="radio"]');
      radios.forEach(function (radio) {
        radio.checked = false;
      });

      // Update count after clearing all checkboxes and radios
      updateCount();
    }

    // Call updateCount initially to set the initial count
    updateCount();
  });
</script>


<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Function to update filter count
    function updateFilterCount() {
      let totalCount = 0;
      // Loop through count badges and sum up counts
      const countBadges = document.querySelectorAll('.count-badge');
      countBadges.forEach(countBadge => {
        totalCount += parseInt(countBadge.dataset.val);
      });
      // Update filter count
      const filterCountElement = document.querySelector('.filter-btn span');
      if (filterCountElement) {
        if (totalCount > 0) {
          filterCountElement.textContent = `(${totalCount})`;
        } else {
          filterCountElement.textContent = ''; // Hide the count if it's zero
        }
      }
    }

    // Hide filter count initially
    const filterCountElement = document.querySelector('.filter-btn span');
    if (filterCountElement) {
      filterCountElement.textContent = '';
    }

    // Listen for click event on Apply Filter button
    const applyFilterButton = document.getElementById('applyFilterButton');
    if (applyFilterButton) {
      applyFilterButton.addEventListener('click', function() {
        // Call updateFilterCount function after applying filter
        updateFilterCount();
      });
    }

    // Listen for click event on Clear All button
    const clearAllButton = document.getElementById('clearAllButton');
    if (clearAllButton) {
      clearAllButton.addEventListener('click', function() {
        // Reset filter count to zero
        const filterCountElement = document.querySelector('.filter-btn span');
        if (filterCountElement) {
          filterCountElement.textContent = '';
        }
      });
    }
  });
</script>


