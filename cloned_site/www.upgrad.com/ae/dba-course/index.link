      function updateCount() {
        // console.log('Updating count...');
  
        var countSpans = document.querySelectorAll('.count-badge');
        countSpans.forEach(function (countSpan) {
          var tabId = countSpan.getAttribute('data-tab');
          //console.log('Tab ID:', tabId);
  
          // Select checkboxes and radios within the current tab
          var tabCheckboxes = document.querySelectorAll('#v-pills-' + tabId + ' input[type="checkbox"]:checked');
          var tabRadios = document.querySelectorAll('#v-pills-' + tabId + ' input[type="radio"]:checked');
          // console.log('Tab Checkboxes:', tabCheckboxes.length);
          // console.log('Tab Radios:', tabRadios.length);
  
          var totalCount = tabCheckboxes.length + tabRadios.length;
          //console.log('Total Count:', totalCount);
  
          countSpan.textContent = '(' + totalCount + ')';
        });
      }
  
      // Function to clear all checkboxes and radios and reset the count
      function clearAll() {
        var checkboxes = document.querySelectorAll('input[type="checkbox"]');
        checkboxes.forEach(function (checkbox) {
          checkbox.checked = false;
        });
  
        var radios = document.querySelectorAll('input[type="radio"]');
        radios.forEach(function (radio) {
          radio.checked = false;
        });
  
        // Update count after clearing all checkboxes and radios
        updateCount();
      }
  
      // Call updateCount initially to set the initial count
      updateCount();
    });
