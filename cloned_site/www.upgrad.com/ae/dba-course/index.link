    <script>
     document.addEventListener("DOMContentLoaded", function() {
       // Get all elements with the class "updateable"
       var updateableElements = document.querySelectorAll(".updateable");
  
       // Function to update the data-val attribute based on the count
       function updateDataVal(element) {
         var count = parseInt(element.textContent.match(/\((\d+)\)/)[1]);
         element.dataset.val = count;
       }
  
       // Initial update for each element
       updateableElements.forEach(function(element) {
         updateDataVal(element);
       });
  
       // Create a MutationObserver to watch for changes in the DOM
       var observer = new MutationObserver(function(mutationsList) {
         mutationsList.forEach(function(mutation) {
           // Check if the content of the observed element has changed
           if (mutation.type === 'childList' && mutation.target.classList.contains('updateable')) {
             // Update the data-val attribute
             updateDataVal(mutation.target);
           }
         });
       });
  
       // Configure the observer to watch for changes to the child nodes
       var config = { childList: true, subtree: true };
  
       // Start observing changes on each updateable element
       updateableElements.forEach(function(element) {
         observer.observe(element, config);
       });
     });
  </script>
  
